<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".RegistreActivity2"
    layout_width="wrap_cont"
    layout_height="">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:minHeight="?attr/actionBarSize"
        android:background="#92D4D4"
        app:titleTextColor="@color/black"
        app:title="@string/app_name"
        android:elevation="20dp"
        app:popupTheme="@style/ThemeOverlay.AppCompat.ActionBar"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_margin="10dp"
        android:gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/nomina"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="29dp"
                android:layout_height="87dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/programador" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="29dp"
                android:layout_height="34dp"
                android:layout_weight="1"
                android:text="Empleo" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:text="$" />

            <EditText
                android:id="@+id/editTextNumber"
                android:layout_width="50dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="number" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/arriendo"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="21dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:srcCompat="@drawable/casa"
                tools:layout_editor_absoluteX="3dp"
                tools:layout_editor_absoluteY="683dp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="29dp"
                android:layout_height="34dp"
                android:layout_weight="1"
                android:text="Arriendo" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:text="$" />

            <EditText
                android:id="@+id/editTextNumber1"
                android:layout_width="50dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="number" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/prestamos"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="29dp"
                android:layout_height="87dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ingresos" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="29dp"
                android:layout_height="34dp"
                android:layout_weight="1"
                android:text="Prestamos/intereses" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:text="$" />

            <EditText
                android:id="@+id/editTextNumber3"
                android:layout_width="50dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="number" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ventas"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="29dp"
                android:layout_height="87dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ventas" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="29dp"
                android:layout_height="34dp"
                android:layout_weight="1"
                android:text="Ventas" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:text="$" />

            <EditText
                android:id="@+id/editTextNumber4"
                android:layout_width="50dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:ems="10"
                android:inputType="number" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_margin="5dp">

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="4"
            android:backgroundTint="#92D4D4"
            android:text="Button" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="4"
            android:elevation="20dp"
            android:backgroundTint="#92D4D4"
            android:text="Button" />

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/floatingActionButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:clickable="true"
            android:tint="#FFFEFF"
            app:backgroundTint="#92D4D4"
            app:elevation="10dp"
            app:srcCompat="@android:drawable/ic_input_add" />
    </LinearLayout>

</LinearLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>